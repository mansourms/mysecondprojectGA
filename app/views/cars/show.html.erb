<h1> information about Car</h1>
<div class="row">
<div class="col s3">
<label > name:</label>
<%= @info_car.name %>
</div>

<br>
<label> year:</label>
<%= @info_car.year%>
<br>
<label> color:</label>
<%= @info_car.color %>
<br>
<label> style:</label>
<%= @info_car.style %>
</div>

<br>

<% @info_car.images.each do |img| %>

 <a href=<%= image_path(img) %>>
 <%= image_tag  img.url, :class=>"img"%>
 </a>
 
<% end %>
<br>
<%= link_to "Edit", edit_car_path(@info_car) %>
<%= link_to "delete",car_path(@info_car), method: :delete %>
